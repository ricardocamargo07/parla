{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "fideloper/proxy": "~4.0",
        "jenssegers/date": "^3.2",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "~1.0",
        "league/commonmark": "^0.17.0",
        "pragmarx/coollection": "^0.5.2",
        "pragmarx/version": "^0.2.7",
        "pragmarx/yaml": "^0.1.6",
        "spatie/laravel-tags": "^2.0",
        "webuni/commonmark-attributes-extension": "dev-master"
    },
    "require-dev": {
        "brainmaestro/composer-git-hooks": "^2.4",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "~1.1",
        "phpunit/phpunit": "~7.0",
        "symfony/thanks": "~1.0"
    },
    "repositories": [
        {
            "url": "https://github.com/jdavidbakr/commonmark-attributes-extension",
            "type": "git"
        }
    ],
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        },

        "hooks": {
            "post-merge": "composer install"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "platform": {
            "php": "7.1.3"
        },
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
